{
  "name": "@canvas-panel/slideshow",
  "version": "0.1.2",
  "description": "canvas-panel-slideshow React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "prepublish": "NODE_ENV=production npm run build",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js"
    }
  },
  "dependencies": {
    "@canvas-panel/core": "0.1.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-flip-move": "^3.0.2",
    "react-transition-group": "3.0.0"
  },
  "peerDependencies": {
    "immutability-helper": "^2.6.5 || ^3.0.0",
    "react": "16.x"
  },
  "devDependencies": {
    "immutability-helper": "3.0.0",
    "nwb": "0.23.0",
    "nwb-sass": "0.9.0",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Digirati",
  "homepage": "https://digirati.com/",
  "license": "MIT",
  "repository": "https://github.com/digirati-co-uk/canvas-panel/tree/master/packages/canvas-panel-slideshow"
}
